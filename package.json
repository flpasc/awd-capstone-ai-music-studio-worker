{
  "name": "awd-capstone-ai-music-studio-worker",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "nodemon --watch src --ext ts,js --exec 'ts-node --transpile-only src/main.ts'",
    "prepare": "husky",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "lint": "eslint \"src/**/*.{ts,js}\" --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf node_modules dist; npm run docker:network:remove; docker compose down -v",
    "clean:storage": "docker compose down -v",
    "docker:network:create": "docker network create ai-studio-shared-network",
    "docker:network:remove": "docker network rm ai-studio-shared-network",
    "modules:update:running": "docker compose stop backend && npm run modules:update:stopped && docker compose up -d backend",
    "modules:update:stopped": "docker compose build backend && docker compose run --rm --no-deps backend npm ci",
    "modules:update": "docker compose ps -q backend | grep -q . && npm run modules:update:running || npm run modules:update:stopped",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flpasc/awd-capstone-ai-music-studio-worker.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flpasc/awd-capstone-ai-music-studio-worker/issues"
  },
  "homepage": "https://github.com/flpasc/awd-capstone-ai-music-studio-worker#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.879.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.0"
  }
}
